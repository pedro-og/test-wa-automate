#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@open-wa/wa-automate-socket-client/node_modules/uuid/dist/bin/uuid" "$@"
  ret=$?
else 
  node  "$basedir/../@open-wa/wa-automate-socket-client/node_modules/uuid/dist/bin/uuid" "$@"
  ret=$?
fi
exit $ret
